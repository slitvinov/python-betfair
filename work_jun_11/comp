                    scriptDownloaded(script);
                };
                script.setAttribute('charset', 'utf-8');
                script.src = filename;
                inProgressDownloads++;
                if ((inProgressDownloads < concurrentLimit) && seedScriptDownloaded) {
                    js_init();
                }
                document.getElementsByTagName('head')[0].appendChild(script);
            }

